{
    "npm.packageManager": "yarn",
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "cSpell.ignoreWords": [
        "SLNSW",
        "app",
        "base",
        "bbox",
        "collection",
        "identifier",
        "iiif",
        "iiif identifier",
        "mapbox",
        "openseadragon",
        "react",
        "slnsw",
        "url",
        "zoomable"
    ],
    "files.associations": {
        "*.glsl.js": "glsl"
    }
}