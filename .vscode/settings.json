{
    "npm.enableScriptExplorer": true,
    "npm.packageManager": "yarn",
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "cSpell.ignoreWords": [
        "bbox",
        "mapbox",
        "zoomable"
    ],
    "files.associations": {
        "*.glsl.js": "glsl"
    }
}